{
  "private": true,
  "name": "preact-rollup",
  "version": "1.0.0",
  "description": "Preact + Rollup template.",
  "main": "src/index.js",
  "author": "Sanshain",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "rollup -c -w",
	 "start": "node ./.esbuild.js",
    "build": "rollup -c",
    "esbuild": "esbuild ./source/index.js --bundle --loader:.js=jsx --minify --outfile=out.js --sourcemap",
    "serve": "serve release"
  },
  "workspaces": [
    "./source/store/*",
    "!source/store/examples/**"
  ],
  "optionalDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@reatom/framework": "^3.4.52",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "babel-plugin-import": "^1.13.3",
    "esbuild": "^0.23.1",
    "esbuild-css-modules-plugin": "^3.1.2",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-ts-paths": "^1.1.3",
    "less": "^4.1.1",
    "preact-render-to-string": "^5.1.19",
    "rimraf": "^3.0.2",
    "rollup": "^4.17.2",
    "rollup-plugin-commonjs-alternate": "^0.8.0",
    "rollup-plugin-css-modules": "^0.1.2",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postcss-modules": "^2.1.1",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "serve": "^11.3.2",
    "stylus": "^0.54.8",
    "tslib": "^2.6.2",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "dependencies": {
    "@reatom/core": "^3.9.0",
    "@reatom/npm-react": "^3.9.0",
    "goober": "^2.0.37",
    "preact": "^10.4.6",
    "preact-jsx-runtime": "^1.2.0",
    "preact-router": "^3.2.1",
    "react-css-modules": "^4.7.11",
    "typescript": "^5.4.5",
    "valtio": "^2.0.0"
  }
