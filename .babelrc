{
  "presets": [
    [
      "@babel/env", {
        "modules": false
      },
		"@babel/react"
    ]
  ],
  "plugins": [
    [
      "@babel/transform-react-jsx",
      {
        "pragma":"h", 
		  "pragmaFrag": "Fragment"
      }
    ],

	[
		"import", 
		{
			"libraryName": "antd",
			"style": true
		}
	],
	["@babel/plugin-proposal-class-properties", { "loose": true }]
	 
  ]

}
